взяти визначення біб;

структура Вектор<Т> {
  довжина: позитивне;
  дані: памʼять<Т>;
}

дія зробити_вектор<Т>(): Вектор<Т> {
  ціль вектор = Вектор<Т> {};
  вектор.довжина = 0;
  вектор.дані = пусто;
  вернути вектор;
}

дія додати_до_вектору<Т>(вектор: комірка<Вектор<Т>>, дані: Т) {
  вектор.дані = перевиділити<Т>(вектор.дані, (вектор.довжина + 1) як ц32);
  вектор.дані[вектор.довжина] = дані;
  вектор.довжина = вектор.довжина + 1;
}

дія забрати_з_вектору<Т>(вектор: комірка<Вектор<Т>>): Т {
  ціль дані = вектор.дані[вектор.довжина - 1];
  вектор.довжина = вектор.довжина - 1;
  вернути дані;
}

дія звільнити_вектор<Т>(вектор: комірка<Вектор<Т>>) {
  звільнити(вектор.дані);
  звільнити(вектор);
}