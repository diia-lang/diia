взяти визначення мавка;

дія старт(): ц32 {
  ціль ВМ = мавка::створити_виконувач();
  ціль результат_взяття_модуля = мавка::взяти_модуль(ВМ, обʼєкт_модуля, ю8"привіт");
  якщо результат_взяття_модуля.помилка != пусто {
    мавка::звільнити_помилку(результат_взяття_модуля.помилка);
  } інакше {
    ціль обʼєкт_модуля = результат_взяття_модуля.обʼєкт_модуля;
    ціль
  }
  мавка::знищити_виконувач(ВМ);
  вернути 0;
}