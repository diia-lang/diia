взяти визначення мавка;

дія старт(): ц32 {
  ціль ВМ = мавка::створити_виконувач();
  мавка::увімкнути_утилізатор(ВМ);
  ціль обʼєкт = мавка::створити_обʼєкт(ВМ);
  ціль результат_виконання_файлу = мавка::виконати_файл(ВМ, ю8"привіт.м", помилка_мавки::адреса);
  якщо результат_виконання_файлу.помилка != пусто {
    мавка::звільнити_помилку(результат_виконання_файлу.помилка);
  }
  мавка::знищити_виконувач(ВМ);
  вернути 0;
}