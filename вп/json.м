js """
м_parse = мДія(
  "parse",
  [мПарм("значення", м_текст)],
  function(args) {
    var value = args[0] ?? null;
    var result = JSON.parse(value);

    function toMavkaValue(v) {
      if (Array.isArray(v)) {
        return v.map(toMavkaValue);
      }
      if (typeof v === "object" && v) {
        return new Map(Object.entries(v).map(([k, v]) => [k, toMavkaValue(v)]));
      }
      return v;
    }

    return toMavkaValue(result);
  }
);
м_stringify = мДія(
  "stringify",
  [мПарм("значення")],
  function(args) {
    var value = args[0] ?? null;

    function replacer(key, value) {
      if (value instanceof Map) {
        return Object.fromEntries(value.entries());
      } else {
        return value;
      }
    }

    return JSON.stringify(value, replacer);
  }
);
"""

макет дія parse(значення текст) щось
макет дія stringify(значення щось) текст

дати parse
дати stringify