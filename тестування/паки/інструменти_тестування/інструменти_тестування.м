дія впасти_якщо_не_рівно(а, б)
  якщо а не рівно б
    впасти "впасти_якщо_не_рівно"
  кінець
кінець

дія впасти_якщо_не_є(а, б)
  якщо а не є б
    впасти "впасти_якщо_не_є"
  кінець
кінець

дія впасти_якщо_списки_не_рівні(а, б)
  якщо а.довжина не рівно б.довжина
    впасти "впасти_якщо_списки_не_рівні (довжина)"
  кінець
  перебрати 0..а.довжина як п
    якщо а[п] не рівно б[п]
      впасти "впасти_якщо_списки_не_рівні (елемент)"
    кінець
  кінець
кінець

дати впасти_якщо_не_рівно
дати впасти_якщо_не_є
дати впасти_якщо_списки_не_рівні