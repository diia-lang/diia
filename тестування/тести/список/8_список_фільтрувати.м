взяти пак інструменти_тестування

значення = [1, "2", так, ні]
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 4)

нове_значення = значення.фільтрувати((ел): ел)
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 3)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, [1, "2", так])

нове_значення = значення.фільтрувати((ел): ел == "2")
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 1)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, ["2"])

нове_значення = значення.фільтрувати((ел): ел == 1)
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 1)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, [1])

нове_значення = значення.фільтрувати((ел): ел == ні)
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 1)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, [ні])

нове_значення = значення.фільтрувати((ел): ел є текст)
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 1)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, ["2"])

нове_значення = значення.фільтрувати((ел): ел є логічне)
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 2)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, [так, ні])

нове_значення = значення.фільтрувати((ел): !ел)
інструменти_тестування.впасти_якщо_не_рівно(нове_значення.довжина, 1)
інструменти_тестування.впасти_якщо_списки_не_рівні(нове_значення, [ні])