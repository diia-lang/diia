взяти пак інструменти_тестування

значення = [1, 2, "3", [=], []]
початкова_довжина = значення.довжина

інструменти_тестування.впасти_якщо_не_рівно(значення[1], 2)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент(0), 1)

якщо видалити значення[1] == ні
  впасти "видалення елемента вернуло ні"
кінець

інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, початкова_довжина - 1)
інструменти_тестування.впасти_якщо_не_рівно(значення[1], "3")
інструменти_тестування.впасти_якщо_не_є(значення.чародія_отримати_елемент(2), словник)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_отримати_елемент(3), список)
інструменти_тестування.впасти_якщо_не_рівно(значення[4], пусто)
інструменти_тестування.впасти_якщо_не_рівно(значення[5], пусто)