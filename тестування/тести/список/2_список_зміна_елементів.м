взяти пак інструменти_тестування

значення = [1, 2, "3", [=], []]

інструменти_тестування.впасти_якщо_не_рівно(значення[1], 2)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент(0), 1)

значення[1] = 33
значення.чародія_змінити_елемент(0, 22)

інструменти_тестування.впасти_якщо_не_рівно(значення[1], 33)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент(0), 22)