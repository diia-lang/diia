взяти пак інструменти_тестування

значення = [1, "2", так, ні]
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 4)

ел = значення.забрати()
інструменти_тестування.впасти_якщо_не_рівно(ел, ні)
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 3)

ел = значення.забрати()
інструменти_тестування.впасти_якщо_не_рівно(ел, так)
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 2)

ел = значення.забрати()
інструменти_тестування.впасти_якщо_не_рівно(ел, "2")
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 1)

ел = значення.забрати()
інструменти_тестування.впасти_якщо_не_рівно(ел, 1)
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 0)

ел = значення.забрати()
інструменти_тестування.впасти_якщо_не_рівно(ел, пусто)
інструменти_тестування.впасти_якщо_не_рівно(значення.довжина, 0)