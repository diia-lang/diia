взяти пак інструменти_тестування

значення = ["а"="б", "в"=10, "г"=так, "ґ"=[=], "д"=[]]

інструменти_тестування.впасти_якщо_не_рівно(значення["в"], 10)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент("а"), "б")

значення["в"] = 20
значення.чародія_змінити_елемент("а", ю"нікод")

інструменти_тестування.впасти_якщо_не_рівно(значення["в"], 20)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент("а"), ю"нікод")
