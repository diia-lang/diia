взяти пак інструменти_тестування

значення = ["а"="б", "в"=10, "г"=так, "ґ"=[=], "д"=[]]

інструменти_тестування.впасти_якщо_не_рівно(значення містить "а", так)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_містить("ґ"), так)
інструменти_тестування.впасти_якщо_не_рівно(значення містить "м", ні)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_містить("є"), ні)
