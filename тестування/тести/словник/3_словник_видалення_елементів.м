взяти пак інструменти_тестування

значення = ["а"="б", "в"=10, "г"=так, "ґ"=[=], "д"=[]]

інструменти_тестування.впасти_якщо_не_рівно(значення["в"], 10)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент("а"), "б")

якщо видалити значення["в"] == ні
  впасти "видалення елемента вернуло ні"
кінець
якщо значення.чародія_видалити_елемент("а") == ні
  впасти "видалення елемента вернуло ні"
кінець

інструменти_тестування.впасти_якщо_не_рівно(значення["в"], пусто)
інструменти_тестування.впасти_якщо_не_рівно(значення.чародія_отримати_елемент("а"), пусто)
