взяти пак інструменти_тестування

значення = ["а"="б", "в"=10, "г"=так, "ґ"=[=], "д"=[]]

інструменти_тестування.впасти_якщо_не_рівно(значення.розмір, 5)
інструменти_тестування.впасти_якщо_не_є(значення.визначення, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.значення, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.видалити, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.очистити, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_містить, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_отримати, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_покласти, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_перебір, Дія)
