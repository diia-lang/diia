взяти пак інструменти_тестування

значення = ["а"="б", "в"=10, "г"=так, "ґ"=[=], "д"=[]]

інструменти_тестування.впасти_якщо_не_рівно(значення.розмір, 5)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_отримати_елемент, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_змінити_елемент, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_видалити_елемент, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.чародія_містить, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.ключі, Дія)
інструменти_тестування.впасти_якщо_не_є(значення.значення, Дія)
