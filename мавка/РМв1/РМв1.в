взяти визначення біб;

секція РМ {
  структура Слово;
  структура Середовище;
  структура Машина;
  структура Обʼєкт;
  структура Місцезнаходження;
  структура Значення;
  структура Результат;
  структура Мавка;

  синонім ЗПусто = 0;
  синонім ЗТак = 1;
  синонім ЗНі = 2;
  синонім ЗЧисло = 3; // дійсне
  синонім ЗСимвол = 4; // п32
  синонім ЗОбʼєкт = 5; // комірка<Обʼєкт>
  синонім ЗСлово = 6; // комірка<Слово> (внутрішнє значення МаМа)

  структура Значення {
    тип: ціле;
    дані: дійсне | комірка<Обʼєкт> | п32 | комірка<Слово>;
  }

  структура Результат {
    успіх: позитивне;
    значення: Значення;
  }

  структура Мавка {
    М: комірка<Машина>;
    отримати_слово: (комірка<Мавка>, памʼять<п8>) -> комірка<Слово>;
    отримати_властивість: (комірка<Мавка>, комірка<Обʼєкт>, комірка<Слово>) -> Значення;
    змінити_властивість: (комірка<Мавка>, комірка<Обʼєкт>, комірка<Слово>, Значення) -> Результат;
    створити_текст: (комірка<Мавка>, памʼять<п8>) -> комірка<Обʼєкт>;
    створити_помилку: (комірка<Мавка>, комірка<Місцезнаходження>, памʼять<п8>) -> комірка<Обʼєкт>;
//    створити_список: (комірка<Мавка>) -> комірка<Обʼєкт>;
//    отримати_елемент_списку: (комірка<Мавка>, комірка<Обʼєкт>, позитивне) -> Значення;
//    змінити_елемент_списку: (комірка<Мавка>, комірка<Обʼєкт>, позитивне, Значення) -> Результат;
//    створити_словник: (комірка<Мавка>) -> комірка<Обʼєкт>;
//    отримати_елемент_словника: (комірка<Мавка>, комірка<Обʼєкт>, Значення) -> Значення;
//    змінити_елемент_словника: (комірка<Мавка>, комірка<Обʼєкт>, Значення, Значення) -> Результат;
    створити_нативну_дію: (комірка<Мавка>, комірка<Слово>, (комірка<Мавка>, комірка<Місцезнаходження>, комірка<Обʼєкт>, позитивне, памʼять<Значення>) -> Результат) -> комірка<Обʼєкт>;
//    створити_структуру: (комірка<Мавка>, комірка<Слово>) -> комірка<Обʼєкт>;
    створити_модуль: (комірка<Мавка>, комірка<Слово>) -> комірка<Обʼєкт>;
//    створити_перебір: (комірка<Мавка>) -> комірка<Обʼєкт>;
    обʼєкт_є_текстом: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є_списком: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є_словником: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є_дією: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є_структурою: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є_модулем: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є_перебором: (комірка<Мавка>, комірка<Обʼєкт>) -> позитивне;
//    обʼєкт_є: (комірка<Мавка>, комірка<Обʼєкт>, комірка<Обʼєкт>) -> позитивне;
    отримати_ю8_обʼєкта_тексту: (комірка<Мавка>, комірка<Обʼєкт>) -> памʼять<п8>;
  }

  структура РезультатЗавантаженняБібліотеки {
    назва: памʼять<п8>;
    мавка: комірка<Мавка>;
  }
}

//зовнішня дія завантажити_розширення_мавки(мавка: комірка<РМ::Мавка>, середовище: комірка<РМ::Середовище>, обʼєкт_модуля: комірка<РМ::Обʼєкт>): РМ::Результат;