взяти визначення бібліотека;

секція мама {
  місцева дія завантажити_інформацію_бібліотеки_мавки(М: комірка<Машина>): комірка<ІнформаціяБібліотекиМавки> {
    ціль інформація_бібліотеки_мавки = виділити<ІнформаціяБібліотекиМавки>();
    інформація_бібліотеки_мавки.кількість_модулів = 2;
    інформація_бібліотеки_мавки.модулі = виділити<ІнформаціяМодуляБібліотекиМавки>(інформація_бібліотеки_мавки.кількість_модулів);
    інформація_бібліотеки_мавки.модулі[0] = ІнформаціяМодуляБібліотекиМавки { ю8"М", завантажити_біб_М, ю8"М" };
    інформація_бібліотеки_мавки.модулі[1] = ІнформаціяМодуляБібліотекиМавки { ю8"читати", завантажити_біб_читати, ю8"читати" };
    вернути інформація_бібліотеки_мавки;
  }
}