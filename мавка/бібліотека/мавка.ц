    дія РМв1_записати_в_байти(обʼєкт: адреса<Обʼєкт>, Р: адреса<Розширення>, дані: памʼять<п8>, розмір: позитивне, зсув: позитивне) {
      мавка::записати_в_байти(обʼєкт як адреса<мавка::Обʼєкт>, Р.В як адреса<мавка::Виконувач>, дані, розмір, зсув);
    }

    дія РМв1_отримати_елементи_словника(обʼєкт: адреса<Обʼєкт>, Р: адреса<Розширення>, вихід: адреса<памʼять<ЕлементСловника>>): позитивне {
      ціль обʼєкт_словника = обʼєкт як адреса<МаМа::ОбʼєктСловника>;
      вихід::вміст = МаМа::виділити<ЕлементСловника>(обʼєкт_словника.елементи.розмір);
      змінна ціль п: позитивне = 0;
      поки п < обʼєкт_словника.елементи.розмір {
        вихід::вміст[п].ключ = обʼєкт_словника.елементи.елементи[п].ключ як адреса<РМв1::Обʼєкт>;
        вихід::вміст[п].значення = обʼєкт_словника.елементи.елементи[п].значення як адреса<РМв1::Обʼєкт>;
        п += 1;
      }
      вернути обʼєкт_словника.елементи.розмір;
    }

    дія РМв1_перевиділити(Р: адреса<Розширення>, значення: памʼять<п8>, новий_розмір: позитивне): памʼять<п8> {
      вернути МаМа::перевиділити<п8>(значення, новий_розмір) як памʼять<п8>;
    }

    дія РМв1_знайти_іменований_аргумент(Р: адреса<Розширення>, іменовані_аргументи: адреса<ІменованіАргументи>, назва: адреса<Назва>, вихід: адреса<адреса<Обʼєкт>>): логічне {
      вернути мавка::знайти_іменований_аргумент(
        Р.В як адреса<мавка::Виконувач>,
        іменовані_аргументи як адреса<мавка::ІменованіАргументи>,
        назва як адреса<мавка::Назва>,
        вихід як адреса<адреса<мавка::Обʼєкт>>
      );
    }

    дія РМв1_записати_іменований_аргумент(Р: адреса<Розширення>, іменовані_аргументи: адреса<ІменованіАргументи>, назва: адреса<Назва>, значення: адреса<Обʼєкт>) {
