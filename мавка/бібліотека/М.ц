взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    місцева дія заповнити_модуль_М(В: адреса<Виконувач>, обʼєкт_модуля: адреса<Обʼєкт>) {
      змінити_властивість(обʼєкт_модуля, В, ю8"Е", створити_число(В, ЧИСЛО_Е));
      змінити_властивість(обʼєкт_модуля, В, ю8"ЛОГ2Е", створити_число(В, ЧИСЛО_ЛОГ2Е));
      змінити_властивість(обʼєкт_модуля, В, ю8"ЛОГ10Е", створити_число(В, ЧИСЛО_ЛОГ10Е));
      змінити_властивість(обʼєкт_модуля, В, ю8"ЛОГ2", створити_число(В, ЧИСЛО_ЛОГ2));
      змінити_властивість(обʼєкт_модуля, В, ю8"ЛОГ10", створити_число(В, ЧИСЛО_ЛОГ10));
      змінити_властивість(обʼєкт_модуля, В, ю8"ПІ", створити_число(В, ЧИСЛО_ПІ));
      змінити_властивість(обʼєкт_модуля, В, ю8"КОРІНЬ2", створити_число(В, ЧИСЛО_КОРІНЬ2));
      змінити_властивість(обʼєкт_модуля, В, ю8"КОРІНЬ1_2", створити_число(В, ЧИСЛО_КОРІНЬ1_2));
    }
  }
}