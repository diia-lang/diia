взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    дія нативна_дія_бібліотеки_мавки_читати(обʼєкт_нативної_дії: адреса<Обʼєкт>, В: адреса<Виконувач>, обʼєкт_я: адреса<Обʼєкт>, аргументи: адреса<Накопичувач<адреса<Обʼєкт>>>, іменовані_аргументи: адреса<ІменованіАргументи>, дані: адреса): Результат {
      ціль значення: памʼять<п8> = пусто;
      ціль довжина_значення = прочитати_з_stdin(ю8"", значення::адреса);
      якщо значення == пусто {
        вернути результат_успіх(пусто);
      } інакше {
        вернути результат_успіх(створити_юнікод_з_Ю8(В, значення));
      }
    }

    місцева дія заповнити_модуль_читати(В: адреса<Виконувач>, обʼєкт_модуля: адреса<Обʼєкт>) {
      обʼєкт_змінити_властивість(обʼєкт_модуля, В, назва_з_Ю8(В, ю8"чародія_виконати"), створити_нативну_дію(В, назва_з_Ю8(В, ю8"читати"), нативна_дія_бібліотеки_мавки_читати, пусто, пусто));
    }
  }
}