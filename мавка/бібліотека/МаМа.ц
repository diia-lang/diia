взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    дія нативна_дія_мавки_отримати_код(обʼєкт_нативної_дії: адреса<мавка::Обʼєкт>, В: адреса<мавка::Виконувач>, обʼєкт_я: адреса<мавка::Обʼєкт>, аргументи: адреса<Накопичувач<адреса<мавка::Обʼєкт>>>, іменовані_аргументи: адреса<Реєстр<адреса<мавка::Назва>, адреса<мавка::Обʼєкт>>>): мавка::Результат {
      вернути мавка::результат_успіх(пусто);
    }

    місцева дія заповнити_модуль_МаМа(В: адреса<Виконувач>, обʼєкт_модуля: адреса<Обʼєкт>) {
      змінити_властивість(обʼєкт_модуля, В, ю8"отримати_код", створити_нативну_дію(В, назва_з_ю8(В, ю8"отримати_код"), нативна_дія_мавки_отримати_код));
    }
  }
}