взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    дія нативна_дія_мавки_отримати_код(предмет_нативної_дії: адреса<Предмет>, В: адреса<Виконувач>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, дані: адреса): Результат {
      вернути результат_успіх(В, пусто);
    }

    місцева дія заповнити_модуль_МаМа(В: адреса<Виконувач>, предмет_модуля: адреса<Предмет>) {
      предмет_змінити_властивість(предмет_модуля, В, назва_з_Ю8(В, ю8сі"отримати_код"), створити_нативну_дію(В, назва_з_Ю8(В, ю8сі"отримати_код"), нативна_дія_мавки_отримати_код, пусто, пусто));
    }
  }
}