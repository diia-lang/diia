взяти визначення МаМа;

секція мама {
  місцева дія створити_обʼєкт_помилка(М: комірка<Машина>, місцезнаходження: комірка<Місцезнаходження>, повідомлення: памʼять<п8>): комірка<Обʼєкт> {
    ціль обʼєкт_помилки = створити_обʼєкт(М, ООбʼєкт);
    обʼєкт_помилки.с = М.структура_помилка;
    обʼєкт_помилки.місцезнаходження_помилки = місцезнаходження;
    змінити_властивість_обʼєкта(обʼєкт_помилки, отримати_або_створити_слово(М, ю8"повідомлення"), Значення { ЗОбʼєкт, дані = створити_обʼєкт_текст(М, повідомлення) });
    вернути обʼєкт_помилки;
  }

  місцева дія створити_структуру_помилка(М: комірка<Машина>): комірка<Обʼєкт> {
    ціль слово = отримати_або_створити_слово(М, ю8"Помилка");
    ціль обʼєкт_структури = створити_обʼєкт_структура(М, слово, М.структура_обʼєкт);
    вернути зробити_безсмертним(М, обʼєкт_структури);
  }

  місцева дія створити_помилку_П2_неможливо_змінити_властивість(М: комірка<Машина>, місцезнаходження: комірка<Місцезнаходження>, слово_назви: комірка<Слово>, значення: Значення): комірка<Обʼєкт> {
