взяти визначення МаМа;

секція мама {
  місцева дія результат_значення(значення: Значення): Результат {
    вернути Результат { успіх = 1, значення = значення };
  }

  місцева дія результат_помилка(значення: Значення): Результат {
    вернути Результат { успіх = 0, значення = значення };
  }

  місцева дія результат_помилка_обʼєкт(обʼєкт: комірка<Обʼєкт>): Результат {
    вернути Результат { успіх = 0, значення = Значення { ЗОбʼєкт, дані = обʼєкт } };
  }
}