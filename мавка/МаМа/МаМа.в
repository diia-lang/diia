секція мама {
  структура Властивість {
    слово: комірка<Слово>;
    значення: Значення;
  }

  структура ПараметрДії {
    позиція: ціле;
    слово: комірка<Слово>;
    тип: комірка<Обʼєкт>;
    тип_може_бути_пусто: позитивне;
    має_значення_за_замовчуванням: позитивне;
    значення_за_замовчуванням: Значення;
  }

  структура ПараметрСтруктури {
    позиція: ціле;
    слово: комірка<Слово>;
    тип: комірка<Обʼєкт>;
    тип_може_бути_пусто: позитивне;
    має_значення_за_замовчуванням: позитивне;
    значення_за_замовчуванням: Значення;
  }

  структура Обʼєкт {
    наступний_обʼєкт: комірка<Обʼєкт>;
    помарковано: ціле;
    безсмертний: ціле;
    тип: ціле;
    с: комірка<Обʼєкт>;
    властивості: комірка<карта<комірка<Слово>, Значення>>;
    отримати_властивість: (комірка<Машина>, комірка<Місцезнаходження>, комірка<Обʼєкт>, комірка<Слово>) -> Результат;
    змінити_властивість: (комірка<Машина>, комірка<Місцезнаходження>, комірка<Обʼєкт>, комірка<Слово>, Значення) -> Результат;
    виконати_як_дію: (комірка<Машина>, комірка<Місцезнаходження>, комірка<Обʼєкт>, комірка<вектор<Аргумент>>) -> Результат;
    місцезнаходження_помилки: комірка<Місцезнаходження>; // якщо результат неуспішний
    // тип: ОДія
    дія_слово: комірка<Слово>;
    дія_параметри: комірка<вектор<ПараметрДії>>;
    дія_тип_повернення: комірка<Обʼєкт>;
    дія_тип_повернення_може_бути_пусто: позитивне;
    дія_вказівки: комірка<вектор<Вказівка>>;
    дія_зовнішнє_середовище: комірка<Середовище>;
    дія_я: комірка<Обʼєкт>;
    // тип: ОНативнаДія
    нативна_дія: (комірка<Машина>, комірка<Місцезнаходження>, комірка<Обʼєкт>, комірка<вектор<Аргумент>>) -> Результат;
    нативна_дія_мавки: невідома_комірка;
    // тип: ОСтруктура
    структура_слово: комірка<Слово>;
    структура_предок: комірка<Обʼєкт>;
    структура_параметри: комірка<вектор<ПараметрСтруктури>>;
    структура_кількість_параметрів_від_предка: ціле;
    структура_методи: комірка<карта<комірка<Слово>, комірка<Обʼєкт>>>;
    // тип: ОТекст
    текст_довжина: позитивне;
    текст_значення: памʼять<п32>;
    // тип: ОМодуль
    модуль_слово: комірка<Слово>;
    модуль_взятий_модуль: комірка<ВзятийМодуль>;
    // тип: ОСписок
    список_елементи: комірка<вектор<Значення>>;
    // тип: ОСловник
    словник_елементи: комірка<карта<Значення, Значення>>;
    // ОПеребірСписку
    перебір_списку_список: комірка<Обʼєкт>;
    перебір_списку_позиція: позитивне;
    // ОПеребірСловника
    перебір_словника_словник: комірка<Обʼєкт>;
    перебір_словника_позиція: позитивне;
    // ОПеребірТексту
    перебір_тексту_текст: комірка<Обʼєкт>;
    перебір_тексту_позиція: позитивне;
  }

  місцева дія створити_обʼєкт(М: комірка<Машина>, тип: ціле): комірка<Обʼєкт>;
  місцева дія звільнити_обʼєкт(обʼєкт: комірка<Обʼєкт>);
  місцева дія створити_обʼєкт_текст(М: комірка<Машина>, значення: памʼять<п8>): комірка<Обʼєкт>;
  місцева дія створити_обʼєкт_текст_ю32(М: комірка<Машина>, значення: памʼять<п32>): комірка<Обʼєкт>;
  місцева дія створити_обʼєкт_нативна_дія(М: комірка<Машина>, нативна_дія: (комірка<Машина>, комірка<Місцезнаходження>, комірка<Обʼєкт>, комірка<вектор<Аргумент>>) -> Результат): комірка<Обʼєкт>;
  місцева дія створити_обʼєкт_дія(М: комірка<Машина>, зовнішнє_середовище: комірка<Середовище>, слово: комірка<Слово>, код_тіла: комірка<вектор<Вказівка>>, обʼєкт_типу_повернення: комірка<Обʼєкт>, тип_повернення_може_бути_пусто: позитивне): комірка<Обʼєкт>;
  місцева дія створити_обʼєкт_структура(М: комірка<Машина>, слово_назви: комірка<Слово>, обʼєкт_предка: комірка<Обʼєкт>): комірка<Обʼєкт>;
  місцева дія створити_обʼєкт_список(М: комірка<Машина>): комірка<Обʼєкт>;
  місцева дія створити_обʼєкт_словник(М: комірка<Машина>): комірка<Обʼєкт>;
  місцева дія отримати_властивість_обʼєкта(обʼєкт: комірка<Обʼєкт>, слово: комірка<Слово>): Значення;
  місцева дія змінити_властивість_обʼєкта(обʼєкт: комірка<Обʼєкт>, слово: комірка<Слово>, значення: Значення);
  місцева дія створити_обʼєкт_модуль(М: комірка<Машина>, слово_назви: комірка<Слово>): комірка<Обʼєкт>;
