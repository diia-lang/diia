  }

  місцева дія обʼєкт_поділити(обʼєкт: адреса<Обʼєкт>, М: адреса<Машина>, аргумент: адреса<Обʼєкт>): обʼєкт_або_стан_падіння {
    якщо обʼєкт == пусто {
      увімкнути_стан_падіння(М, пусто);
      вернути виділити_обʼєкт_тексту_помилки_неможливо_поділити_пусто(М);
    }
    ціль обʼєкт_чародії = обʼєкт_отримати_властивість(обʼєкт, М, М.БН.С.чародія_поділити);
    якщо обʼєкт_чародії == пусто {
      увімкнути_стан_падіння(М, пусто);
      вернути виділити_обʼєкт_тексту_помилки_неможливо_виконати_чародію_обʼєкта(М, М.БН.С.чародія_поділити, обʼєкт);
    }
    ціль аргументи = зробити_накопичувач<адреса<Обʼєкт>>();
    покласти_в_накопичувач<адреса<Обʼєкт>>(аргументи::адреса, аргумент);
    ціль результат_виконання = обʼєкт_виконати(обʼєкт_чародії, М, обʼєкт, аргументи::адреса, пусто);
    звільнити_значення_накопичувача<адреса<Обʼєкт>>(аргументи);
    вернути результат_виконання;
  }

  місцева дія обʼєкт_отримати_остачу_ділення(обʼєкт: адреса<Обʼєкт>, М: адреса<Машина>, аргумент: адреса<Обʼєкт>): обʼєкт_або_стан_падіння {
    якщо обʼєкт == пусто {
      увімкнути_стан_падіння(М, пусто);
      вернути виділити_обʼєкт_юнікоду_з_ю8(М, ю8"Неможливо поділити за модулем пусто") як адреса<Обʼєкт>;
    }
    ціль обʼєкт_чародії = обʼєкт_отримати_властивість(обʼєкт, М, М.БН.С.чародія_отримати_остачу_ділення);
    якщо обʼєкт_чародії == пусто {
      увімкнути_стан_падіння(М, пусто);
      вернути виділити_обʼєкт_юнікоду_з_ю8(М, ю8"Вітання ви не можете поділити за модулем це") як адреса<Обʼєкт>;
    }
    ціль аргументи = зробити_накопичувач<адреса<Обʼєкт>>();
    покласти_в_накопичувач<адреса<Обʼєкт>>(аргументи::адреса, аргумент);
    ціль результат_виконання = обʼєкт_виконати(обʼєкт_чародії, М, обʼєкт, аргументи::адреса, пусто);
    звільнити_значення_накопичувача<адреса<Обʼєкт>>(аргументи);
    вернути результат_виконання;
  }

  місцева дія обʼєкт_отримати_частку_ділення(обʼєкт: адреса<Обʼєкт>, М: адреса<Машина>, аргумент: адреса<Обʼєкт>): обʼєкт_або_стан_падіння {
    якщо обʼєкт == пусто {
      увімкнути_стан_падіння(М, пусто);
      вернути виділити_обʼєкт_юнікоду_з_ю8(М, ю8"Неможливо поділити націло пусто") як адреса<Обʼєкт>;
    }
    ціль обʼєкт_чародії = обʼєкт_отримати_властивість(обʼєкт, М, М.БН.С.чародія_отримати_частку_ділення);
    якщо обʼєкт_чародії == пусто {
      увімкнути_стан_падіння(М, пусто);
      вернути виділити_обʼєкт_юнікоду_з_ю8(М, ю8"Вітання ви не можете поділити націло це") як адреса<Обʼєкт>;
    }
    ціль аргументи = зробити_накопичувач<адреса<Обʼєкт>>();
    покласти_в_накопичувач<адреса<Обʼєкт>>(аргументи::адреса, аргумент);
    ціль результат_виконання = обʼєкт_виконати(обʼєкт_чародії, М, обʼєкт, аргументи::адреса, пусто);
    звільнити_значення_накопичувача<адреса<Обʼєкт>>(аргументи);
    вернути результат_виконання;
  }

  місцева дія обʼєкт_піднести_до_степеня(обʼєкт: адреса<Обʼєкт>, М: адреса<Машина>, аргумент: адреса<Обʼєкт>): обʼєкт_або_стан_падіння {
