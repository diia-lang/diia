взяти визначення МаМа;

секція МаМа {
  місцева дія надрукувати_складений_обʼєкт_в_буфер_ю8(складений_обʼєкт: адреса<СкладенийОбʼєкт>, М: адреса<Машина>, буфер_ю8: адреса<БуферЮ8>, глибина: позитивне) {
    додати_до_буфера_ю8(буфер_ю8, ю8"СкладенийОбʼєкт");
  }

  місцева дія створити_складений_обʼєкт(М: адреса<Машина>, тип: адреса<ОбʼєктСтруктури>): адреса<СкладенийОбʼєкт> {
    ціль складений_обʼєкт = виділити<СкладенийОбʼєкт>();

    складений_обʼєкт.наступний_обʼєкт = М.утилізатор.початок_обʼєктів;
    складений_обʼєкт.позначено = ні;
    складений_обʼєкт.тип = тип;
    М.утилізатор.початок_обʼєктів = складений_обʼєкт як адреса<Обʼєкт>;
    М.утилізатор.кількість_обʼєктів = М.утилізатор.кількість_обʼєктів + 1;

    складений_обʼєкт.властивості = зробити_реєстр<адреса<Назва>, адреса<Обʼєкт>>(перевірити_чи_назви_рівні);

    вернути складений_обʼєкт;
  }
}