структура АСДДаніВзяти {
  тип: адреса<АСДІдентифікатор>;
  довжина_шляху: позитивне;
  шлях: памʼять<адреса<АСДІдентифікатор>>;
};

структура ТекстКоду {
  шлях: памʼять<п8>;
  значення: памʼять<п8>;
};

структура ПомилкаРозборуМавки {
  місцезнаходження: адреса<Місцезнаходження>;
  повідомлення: памʼять<п8>;
};

структура РезультатРозборуМавки {
  успіх: позитивне;
  тіло: адреса<СписокАСДЗначень>;
  помилка: адреса<ПомилкаРозборуМавки>;
};

зовнішня дія розібрати_мавку(текст_коду: адреса<ТекстКоду>): адреса<РезультатРозборуМавки>;
