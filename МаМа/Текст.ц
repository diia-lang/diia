взяти визначення МаМа;

секція МаМа {
  місцева дія надрукувати_обʼєкт_текст(М: адреса<Машина>, обʼєкт: адреса<Обʼєкт>) {
    надрукувати_ю32(обʼєкт.дані як Ю32);
  }

  дія додати_обʼєкт_тексту(я: адреса<Обʼєкт>, М: адреса<Машина>, середовище: адреса<Середовище>, аргумент: адреса<Обʼєкт>): адреса<Обʼєкт> {
    увімкнути_стан_помилки(М);
    вернути пусто; // потім: вернути текст
  }

  місцева дія виділити_обʼєкт_тексту(М: адреса<Машина>, значення: Ю32): адреса<Обʼєкт> {
    ціль обʼєкт_тексту = виділити_обʼєкт(М, М.обʼєкт_структури_текст);
    обʼєкт_тексту.дані = виділити_копію_ю32(значення);
    обʼєкт_тексту.додати = додати_обʼєкт_тексту;
    обʼєкт_тексту.виконати_ = пусто;
    вернути обʼєкт_тексту;
  }

  місцева дія виділити_обʼєкт_тексту_з_ю8(М: адреса<Машина>, значення: Ю8): адреса<Обʼєкт> {
    ціль обʼєкт_тексту = виділити_обʼєкт(М, М.обʼєкт_структури_текст);
    обʼєкт_тексту.дані = перетворити_ю8_на_ю32(значення);
    обʼєкт_тексту.додати = додати_обʼєкт_тексту;
    обʼєкт_тексту.виконати_ = пусто;
    вернути обʼєкт_тексту;
  }
}