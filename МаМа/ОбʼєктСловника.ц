  }

  місцева дія обʼєкт_словника_отримати_остачу_ділення(обʼєкт_словника: адреса<ОбʼєктСловника>, М: адреса<Машина>, аргумент: адреса<Обʼєкт>): обʼєкт_або_стан_падіння {
