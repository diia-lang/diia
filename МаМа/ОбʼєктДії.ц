взяти визначення МаМа;

секція МаМа {
  місцева дія надрукувати_обʼєкт_дії_в_буфер_ю8(обʼєкт_дії: адреса<ОбʼєктДії>, М: адреса<Машина>, буфер_ю8: адреса<БуферЮ8>, глибина: позитивне) {
    додати_до_буфера_ю8(буфер_ю8, ю8"<дія");
    якщо обʼєкт_дії.назва != пусто {
      додати_до_буфера_ю8(буфер_ю8, ю8" ");
      додати_до_буфера_ю8(буфер_ю8, обʼєкт_дії.назва.значення);
    }
    додати_до_буфера_ю8(буфер_ю8, ю8">");
  }

  місцева дія створити_дію(М: адреса<Машина>, назва: адреса<Назва>, параметри: адреса<Реєстр<адреса<Назва>, Параметр>>, середовище_створення: адреса<Середовище>, код: адреса<Код>): адреса<ОбʼєктДії> {
    ціль обʼєкт_дії = виділити<ОбʼєктДії>();
    обʼєкт_дії.наступний_обʼєкт = М.утилізатор.початок_обʼєктів;
    обʼєкт_дії.позначено = ні;
    обʼєкт_дії.тип = М.обʼєкт_структури_Дія;

    обʼєкт_дії.назва = назва;
    обʼєкт_дії.параметри = параметри;
    обʼєкт_дії.середовище_створення = середовище_створення;
    обʼєкт_дії.код = код;

    М.утилізатор.початок_обʼєктів = обʼєкт_дії як адреса<Обʼєкт>;
    М.утилізатор.кількість_обʼєктів = М.утилізатор.кількість_обʼєктів + 1;
    вернути обʼєкт_дії;
  }
}