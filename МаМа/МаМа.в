взяти визначення МаМа/допоміжне/біб;
взяти визначення МаМа/допоміжне/карта;
взяти визначення МаМа/допоміжне/стопка;
взяти визначення МаМа/допоміжне/вектор;
взяти визначення МаМа/розбирач;

секція МаМа {
  синонім ВВизначити = 1; // [обʼєкт](позиція_назви:п32){} -> []
  синонім ВЗвернутись = 2; // [](позиція_назви:п32){} -> [обʼєкт]
  синонім ВВиконати = 3; // [обʼєкт](){-НА} -> [результат]
  синонім ВОтримати = 4; // [обʼєкт](позиція_назви:п32){} -> [результат]
  синонім ВЗмінити = 5; // [обʼєкт, значення](позиція_назви:п32){} -> []
  синонім ВПокластиЧисло = 6; // [](позиція_числа:п32){} -> []
  синонім ВПокластиТекст = 7; // [](позиція_тексту:п32){} -> []
  синонім ВПокластиЮнікод = 8; // [](позиція_юнікоду:п32){} -> []
  синонім ВСтворитиДію = 9; // ...
  синонім ВСтворитиСтруктуру = 10; // ...
  синонім ВСтворитиСловник = 11; // ...
  синонім ВСтворитиСписок = 12; // ...
  синонім ВДодати = 13; // [а, б](){} -> [результат]

  структура Код;
  структура Назва;

  структура Код {
    розмір_вказівок: п32;
    вказівки: памʼять<п8>;
    кількість_назв: п32;
    назви: памʼять<адреса<Назва>>;
    кількість_чисел: п32;
    числа: памʼять<д64>;
    кількість_текстів: п32;
    тексти: памʼять<памʼять<п32>>;
    кількість_юнікодів: п32;
    юнікоди: памʼять<памʼять<п32>>;
    кількість_підкодів: п32;
    підкоди: памʼять<адреса<Код>>;
  }

  дія виділити_код(): адреса<Код>;
  дія додати_назву_до_коду(код: адреса<Код>, назва: адреса<Назва>): п32;
  дія додати_число_до_коду(код: адреса<Код>, значення: д64): п32;
  дія додати_текст_до_коду(код: адреса<Код>, текст: памʼять<п32>): п32;
  дія додати_юнікод_до_коду(код: адреса<Код>, юнікод: памʼять<п32>): п32;
  дія додати_вказівку_до_коду(код: адреса<Код>, вказівка: п8): п32;
  дія додати_вказівку_до_коду_з_аргументом_п32(код: адреса<Код>, вказівка: п8, аргумент: п32): п32;
  дія прочитати_аргумент_вказівки_п32(код: адреса<Код>, позиція: п32): п32;

  структура Назва {
    значення: Ю8;
  }

  дія виділити_назву(значення: Ю8): адреса<Назва>;
  дія перевірити_чи_назви_рівні(назва1: адреса<Назва>, назва2: адреса<Назва>): логічне;
}

секція МаМа {
  структура Обʼєкт;
  структура Машина;

  синонім ДаніОбʼєктаЧисла = д64;
  синонім ДаніОбʼєктаТексту = памʼять<п32>;
  синонім ДаніОбʼєктаЮнікоду = Ю32;

  структура Обʼєкт {
    обʼєкт_структури: адреса<Обʼєкт>;
    властивості: Карта<адреса<Назва>, адреса<Обʼєкт>>;
    дані: ДаніОбʼєктаЧисла | ДаніОбʼєктаТексту | ДаніОбʼєктаЮнікоду;
    додати: () -> адреса<Обʼєкт>;
  }

  дія виділити_обʼєкт(М: адреса<Машина>, обʼєкт_структури: адреса<Обʼєкт>): адреса<Обʼєкт>;
  дія виділити_обʼєкт_числа(М: адреса<Машина>, значення: д64): адреса<Обʼєкт>;

  структура БазаНазв {
    _дані: Вектор<адреса<Назва>>;
    предок: адреса<Назва>;
  }

  дія зробити_базу_назв(): БазаНазв;
  дія знайти_або_виділити_й_додати_назву(БН: адреса<БазаНазв>, значення: Ю8): адреса<Назва>;

  структура Машина {
    БН: БазаНазв;
    стопка: Стопка<адреса<Обʼєкт>>;
    обʼєкт_структури_обʼєкт: адреса<Обʼєкт>;
    обʼєкт_структури_структура: адреса<Обʼєкт>;
    обʼєкт_структури_число: адреса<Обʼєкт>;
  }

  дія виділити_машину(): адреса<Машина>;
  дія виконати_код(М: адреса<Машина>, код: адреса<Код>);
}