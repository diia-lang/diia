        надрукувати_ю8(ю8" Є");
      } інакше якщо вказівка == ВНеЄ {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" НеЄ");
      } інакше якщо вказівка == ВВідʼємне {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Відʼємне");
      } інакше якщо вказівка == ВЧислове {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Числове");
      } інакше якщо вказівка == ВЗаперечити {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Заперечити");
      }  інакше якщо вказівка == ВЗбільшити {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Збільшити");
      }  інакше якщо вказівка == ВЗбільшитиПопереднє {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ЗбільшитиПопереднє");
      }  інакше якщо вказівка == ВЗменшити {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Зменшити");
      } інакше якщо вказівка == ВПочатиСпробу {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ПочатиСпробу");
        п += 1;
      } інакше якщо вказівка == ВПочатиСпробуЗНазвою {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ПочатиСпробуЗНазвою");
        п += 1;
        п += 1;
      } інакше якщо вказівка == ВПеребратиДалі {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ПеребратиДалі");
      } інакше якщо вказівка == ВВпасти {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Впасти");
      } інакше якщо вказівка == ВЗакінчитиСпробу {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ЗакінчитиСпробу");
        п += 1;
      } інакше якщо вказівка == ВВзяти {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Взяти");
        п += 1;
      } інакше якщо вказівка == ВДати {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Дати");
        п += 1;
      } інакше якщо вказівка == ВПокластиРеєстрАргументівЗНазвами {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ПокластиРеєстрАргументівЗНазвами");
      } інакше якщо вказівка == ВЗареєструватиАргументЗНазвою {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ЗареєструватиАргументЗНазвою");
        п += 1;
      } інакше якщо вказівка == ВПокластиРеєстрПараметрів {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ПокластиРеєстрПараметрів");
      } інакше якщо вказівка == ВЗареєструватиПараметр {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ЗареєструватиПараметр");
        п += 1;
        п += 1;
      } інакше якщо вказівка == ВТриматиКод {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ТриматиКод");
        п += 1;
      } інакше якщо вказівка == ВНакопичитиНазву {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" НакопичитиНазву");
        п += 1;
      } інакше якщо вказівка == ВТриматиНазву {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ТриматиНазву");
        п += 1;
      } інакше якщо вказівка == ВСтрибнути {
        вивести_п64(п64(п));
        вивести_ю8(ю8" Стрибнути ");
        п += 1;
        надрукувати_п64(п64(код.накопичувач_вказівок.дані[п]));
      } інакше якщо вказівка == ВСтрибнутиЯкщоНі {
        вивести_п64(п64(п));
        вивести_ю8(ю8" СтрибнутиЯкщоНі ");
        п += 1;
        надрукувати_п64(п64(код.накопичувач_вказівок.дані[п]));
      } інакше якщо вказівка == ВСтрибнутиЯкщоНіЗалишивши {
        вивести_п64(п64(п));
        вивести_ю8(ю8" СтрибнутиЯкщоНіЗалишивши ");
        п += 1;
        надрукувати_п64(п64(код.накопичувач_вказівок.дані[п]));
      } інакше якщо вказівка == ВСтрибнутиЯкщоТак {
        вивести_п64(п64(п));
        вивести_ю8(ю8" СтрибнутиЯкщоТак ");
        п += 1;
        надрукувати_п64(п64(код.накопичувач_вказівок.дані[п]));
      } інакше якщо вказівка == ВСтрибнутиЯкщоТакЗалишивши {
        вивести_п64(п64(п));
        вивести_ю8(ю8" СтрибнутиЯкщоТакЗалишивши ");
        п += 1;
        надрукувати_п64(п64(код.накопичувач_вказівок.дані[п]));
      } інакше якщо вказівка == ВДублювати {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Дублювати");
      } інакше якщо вказівка == ВДублюватиПопереднє {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" ДублюватиПопереднє");
      } інакше якщо вказівка == ВЗабрати {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Забрати");
      } інакше якщо вказівка == ВВернути {
        вивести_п64(п64(п));
        надрукувати_ю8(ю8" Вернути");
      } інакше {
        вивести_п64(п64(п));
        вивести_ю8(ю8" Невідома вказівка ");
        надрукувати_п64(п64(вказівка));
      }
      п += 1;
    }
  }

  місцева дія знайти_місцезнаходження_вказівки(код: адреса<Код>, позиція_вказівки: п32): адреса<МісцезнаходженняВказівки> {
    змінна ціль позиція: п32 = 0;
    поки позиція < код.накопичувач_місцезнаходжень_вказівок.розмір {
      якщо код.накопичувач_місцезнаходжень_вказівок.дані[позиція].позиція_вказівки == позиція_вказівки {
        вернути код.накопичувач_місцезнаходжень_вказівок.дані[позиція]::адреса;
      }
      позиція = позиція + 1;
    }
    вернути пусто;
  }
}