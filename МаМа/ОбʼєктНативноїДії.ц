взяти визначення МаМа;

секція МаМа {
  місцева дія надрукувати_обʼєкт_нативної_дії_в_буфер_ю8(обʼєкт_нативної_дії: адреса<ОбʼєктНативноїДії>, М: адреса<Машина>, буфер_ю8: адреса<БуферЮ8>, глибина: позитивне) {
    додати_до_буфера_ю8(буфер_ю8, ю8"<нативна дія>");
  }

  місцева дія створити_нативну_дію(М: адреса<Машина>, дія_виконання: ТипНативноїДії): адреса<ОбʼєктНативноїДії> {
    ціль обʼєкт_нативної_дії = виділити<ОбʼєктНативноїДії>();
    обʼєкт_нативної_дії.наступний_обʼєкт = М.утилізатор.початок_обʼєктів;
    обʼєкт_нативної_дії.позначено = ні;
    обʼєкт_нативної_дії.тип = М.обʼєкт_структури_НативнаДія;

    обʼєкт_нативної_дії.властивості = зробити_реєстр<адреса<Назва>, адреса<Обʼєкт>>(перевірити_чи_назви_рівні);
    обʼєкт_нативної_дії.дія_виконання = дія_виконання;

    М.утилізатор.початок_обʼєктів = обʼєкт_нативної_дії як адреса<Обʼєкт>;
    М.утилізатор.кількість_обʼєктів = М.утилізатор.кількість_обʼєктів + 1;
    вернути обʼєкт_нативної_дії;
  }

  місцева дія обʼєкт_нативної_дії_виконати(обʼєкт_нативної_дії: адреса<ОбʼєктНативноїДії>, М: адреса<Машина>, обʼєкт_я: адреса<Обʼєкт>, аргументи: адреса<Накопичувач<адреса<Обʼєкт>>>, іменовані_аргументи: адреса<Реєстр<адреса<Назва>, адреса<Обʼєкт>>>): адреса_або_стан_помилки<Обʼєкт> {
    вернути обʼєкт_нативної_дії.дія_виконання(обʼєкт_нативної_дії, М, обʼєкт_я, аргументи, іменовані_аргументи);
  }
}