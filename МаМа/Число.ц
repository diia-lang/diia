взяти визначення МаМа;

секція МаМа {
  місцева дія надрукувати_обʼєкт_число(М: адреса<Машина>, обʼєкт: адреса<Обʼєкт>) {
    надрукувати_д64(обʼєкт.дані як д64);
  }

  дія додати_обʼєкт_числа(я: адреса<Обʼєкт>, М: адреса<Машина>, середовище: адреса<Середовище>, аргумент: адреса<Обʼєкт>): адреса<Обʼєкт> {
    якщо аргумент == пусто {
      увімкнути_стан_помилки(М, 0);
      вернути пусто; // потім: вернути текст
    }
    якщо аргумент.обʼєкт_структури != М.обʼєкт_структури_число {
      увімкнути_стан_помилки(М, 0);
      вернути пусто; // потім: вернути текст
    }
    вернути виділити_обʼєкт_числа(М, (я.дані як д64) + (аргумент.дані як д64));
  }

  місцева дія виділити_обʼєкт_числа(М: адреса<Машина>, значення: д64): адреса<Обʼєкт> {
    ціль обʼєкт_числа = виділити_обʼєкт(М, М.обʼєкт_структури_число);
    обʼєкт_числа.дані = значення;
    обʼєкт_числа.додати = додати_обʼєкт_числа;
    обʼєкт_числа.виконати_ = пусто;
    вернути обʼєкт_числа;
  }
}