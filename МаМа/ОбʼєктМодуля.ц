  }

  місцева дія обʼєкт_модуля_зсунути_вправо_зі_заповненням(обʼєкт_модуля: адреса<ОбʼєктМодуля>, М: адреса<Машина>, аргумент: адреса<Обʼєкт>): обʼєкт_або_стан_падіння {
