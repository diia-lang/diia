  }

  місцева дія обʼєкт_модуля_заперечити(обʼєкт_модуля: адреса<ОбʼєктМодуля>, М: адреса<Машина>): обʼєкт_або_стан_падіння {
    вернути М.обʼєкт_логічного_ні як адреса<Обʼєкт>;
  }
}