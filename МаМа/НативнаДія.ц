взяти визначення МаМа;

секція МаМа {
  місцева дія надрукувати_обʼєкт_нативної_дії_в_буфер_ю8(М: адреса<Машина>, буфер_ю8: адреса<БуферЮ8>, обʼєкт: адреса<Обʼєкт>) {
    додати_до_буфера_ю8(буфер_ю8, ю8"<нативна дія>");
  }

  місцева дія виділити_обʼєкт_нативної_дії(М: адреса<Машина>, дія_виконання: ДаніОбʼєктаНативноїДії): адреса<Обʼєкт> {
    ціль обʼєкт_дії = виділити_обʼєкт(М, М.обʼєкт_структури_НативнаДія);
    обʼєкт_дії.дані = дія_виконання;
    вернути обʼєкт_дії;
  }
}