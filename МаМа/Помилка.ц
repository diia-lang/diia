    ціль обʼєкт_тексту = виділити_обʼєкт_юнікоду_з_ю8(М, ю8);
    звільнити(ю8 як адреса);
    звільнити_значення_накопичувача_ю8(накопичувач_ю8);
    звільнити(назва_чародії_ю8 як адреса);
    вернути обʼєкт_тексту як адреса<Обʼєкт>;
  }

  місцева дія виділити_обʼєкт_тексту_помилки_пропущено_аргумент(М: адреса<Машина>, назва: адреса<Назва>): адреса<Обʼєкт> {
    ціль назва_ю8: Ю8 = пусто;
    якщо виділити_ю8_з_назви(М, назва, назва_ю8::адреса) == ні {
      ПОМЕРТИ(ю8"Не вдалось отримати ю8 з назви");
    }
    ціль накопичувач_ю8 = зробити_накопичувач_ю8();
    покласти_в_накопичувач_ю8(накопичувач_ю8::адреса, ю8"Пропущено аргумент \"");
    покласти_в_накопичувач_ю8(накопичувач_ю8::адреса, назва_ю8);
    покласти_в_накопичувач_ю8(накопичувач_ю8::адреса, ю8"\"");
    ціль ю8 = виділити_ю8_з_накопичувача_ю8(накопичувач_ю8::адреса);
    ціль обʼєкт_тексту = виділити_обʼєкт_юнікоду_з_ю8(М, ю8);
    звільнити(ю8 як адреса);
    звільнити_значення_накопичувача_ю8(накопичувач_ю8);
    звільнити(назва_ю8 як адреса);
    вернути обʼєкт_тексту як адреса<Обʼєкт>;
  }
}