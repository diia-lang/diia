взяти визначення ./РМв1;

зовнішня дія putchar(value: char): int;

зовнішня дія завантажити_РМв1(Р: адреса<РМв1::Розширення>) {
  змінна ціль п: ц8 = 32;
  поки п < 127 {
    putchar(п);
    п += 1;
  }
  putchar(10);
  РМв1::надрукувати_Ю8(ю8"Hello, world!", Р);
  ціль обʼєкт_словника = РМв1::створити_список(Р);
  РМв1::зберегти_обʼєкт(Р, обʼєкт_словника);
  РМв1::надрукувати_обʼєкт(обʼєкт_словника, Р, 0);
  РМв1::видалити_збережений_обʼєкт(Р);
}