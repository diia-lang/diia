взяти визначення ./РМв1;

зовнішня дія putchar(value: char): int;

зовнішня дія завантажити_РМв1(Р: адреса<РМв1::Розширення>) {
  змінна ціль п: ц8 = 32;
  поки п < 127 {
    putchar(п);
    п += 1;
  }
  putchar(10);
  РМв1::надрукувати_Ю8(ю8"Hello, world!", Р);
  ціль обʼєкт_модуля_розширення = РМв1::отримати_обʼєкт_модуля_розширення(Р);
  ціль обʼєкт_числа_ПІ = РМв1::створити_число(Р, 3.14);
  РМв1::зберегти_обʼєкт(Р, обʼєкт_числа_ПІ);
  ціль результат_зміни_властивості = РМв1::обʼєкт_змінити_властивість(обʼєкт_модуля_розширення, Р, РМв1::назва_з_Ю8(Р, ю8"ПІ"), обʼєкт_числа_ПІ);
  якщо результат_зміни_властивості.падіння != пусто {
    РМв1::надрукувати_Ю8(ю8"Помилка зміни властивості обʼєкта", Р);
    вернути;
  }
  ціль обʼєкт_списку = РМв1::створити_список(Р);
  РМв1::зберегти_обʼєкт(Р, обʼєкт_списку);
  ціль обʼєкт_числа_позиції_0 = РМв1::створити_число(Р, 0);
  РМв1::зберегти_обʼєкт(Р, обʼєкт_числа_позиції_0);
  ціль обʼєкт_значення_позиції_0 = РМв1::створити_число(Р, 42);
  РМв1::зберегти_обʼєкт(Р, обʼєкт_значення_позиції_0);
  ціль результат_зміни_елемента = РМв1::обʼєкт_змінити_елемент(обʼєкт_списку, Р, обʼєкт_числа_позиції_0, обʼєкт_значення_позиції_0);
  РМв1::видалити_збережений_обʼєкт(Р);
  РМв1::видалити_збережений_обʼєкт(Р);
  РМв1::видалити_збережений_обʼєкт(Р);
  РМв1::видалити_збережений_обʼєкт(Р);
  якщо результат_зміни_елемента.падіння != пусто {
    РМв1::надрукувати_Ю8(ю8"Помилка зміни елемента списку", Р);
    вернути;
  }
  РМв1::надрукувати_обʼєкт(обʼєкт_списку, Р, 0);
}