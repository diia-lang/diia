взяти визначення біб;
взяти визначення карта;
взяти визначення стопка;

секція МаМа {
  синонім ВВизначити = 1; // [обʼєкт](позиція_назви:п32){} -> []
  синонім ВЗвернутись = 2; // [](позиція_назви:п32){} -> [обʼєкт]
  синонім ВВиконати = 3; // [обʼєкт](){НА} -> [результат]
  синонім ВОтримати = 4; // [обʼєкт](позиція_назви:п32){} -> [результат]
  синонім ВЗмінити = 5; // [обʼєкт, значення](позиція_назви:п32){} -> []
  синонім ВПокластиЧисло = 6; // [](позиція_числа:п32){} -> []
  синонім ВПокластиТекст = 7; // [](позиція_тексту:п32){} -> []
  синонім ВПокластиЮнікод = 8; // [](позиція_юнікоду:п32){} -> []
  синонім ВДодати = 9; // [а, б](){} -> [результат]

  структура Код;

  структура Код {
    розмір_вказівок: п32;
    вказівки: памʼять<п8>;
    кількість_назв: п32;
    назви: памʼять<памʼять<п8>>;
    кількість_чисел: п32;
    числа: памʼять<д64>;
    кількість_текстів: п32;
    тексти: памʼять<памʼять<п32>>;
    кількість_юнікодів: п32;
    юнікоди: памʼять<памʼять<п32>>;
    кількість_підкодів: п32;
    підкоди: памʼять<комірка<Код>>;
  }

  дія виділити_код(): комірка<Код>;
  дія додати_число_до_коду(код: комірка<Код>, значення: д64): п32;
  дія додати_текст_до_коду(код: комірка<Код>, текст: памʼять<п32>): п32;
  дія додати_юнікод_до_коду(код: комірка<Код>, юнікод: памʼять<п32>): п32;
  дія додати_вказівку_до_коду(код: комірка<Код>, вказівка: п8): п32;
  дія додати_вказівку_до_коду_з_аргументом_п32(код: комірка<Код>, вказівка: п8, аргумент: п32): п32;
}

секція МаМа {
  структура Обʼєкт;
  структура Машина;

  структура Обʼєкт {
    обʼєкт_структури: комірка<Обʼєкт>;
    властивості: Карта<памʼять<п8>, комірка<Обʼєкт>>;
    дані: д64;
  }

  дія виділити_обʼєкт(М: комірка<Машина>, обʼєкт_структури: комірка<Обʼєкт>): комірка<Обʼєкт>;
  дія виділити_обʼєкт_числа(М: комірка<Машина>, значення: д64): комірка<Обʼєкт>;

  структура Машина {
    стопка: Стопка<комірка<Обʼєкт>>;
    обʼєкт_структури_числа: комірка<Обʼєкт>;
  }

  дія виділити_машину(): комірка<Машина>;
  дія виконати_код(М: комірка<Машина>, код: комірка<Код>);
}