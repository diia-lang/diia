секція РМв1 {
  структура Назва;
  структура Обʼєкт;
  структура Середовище;
  структура Виконувач;
  структура Розширення;

  структура Результат {
    успіх: логічне;
    значення: адреса<Обʼєкт>;
  }

  структура Розширення {
    В: адреса<Виконувач>;
    дані: адреса;
  }
}

